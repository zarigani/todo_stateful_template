msgid "%{fn} is reserved"
msgstr "το %{fn} είναι κατειλημένο"

#: lib/gettext/active_record.rb:221
msgid "%{fn} is invalid"
msgstr "το %{fn} είναι άκυρο"

#: lib/gettext/active_record.rb:222
